{
  "private": true,
  "name": "morpheus-ebc",
  "productName": "morpheus-ebc",
  "version": "1.0.0",
  "description": "NVIDIA Morpheus",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "preinstall": "./install-node-rapids.sh"
  },
  "keywords": [],
  "author": {
    "name": "ptaylor",
    "email": "paul.e.taylor@me.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devServer": { "liveReload": false },
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            },
            "devContentSecurityPolicy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' data:; script-src 'self' 'unsafe-eval' 'unsafe-inline' data:; script-src-elem 'self' 'unsafe-eval' 'unsafe-inline' unpkg.com *.unpkg.com data:;"
          }
        ]
      ]
    },
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@timfish/forge-externals-plugin": "0.1.2",
    "@luma.gl/debug": "8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "17.0.1",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-redhat": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "@luma.gl/core": "8.5.10",
    "@deck.gl/core": "8.6.6",
    "@deck.gl/react": "8.6.6",
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-solid-svg-icons": "6.0.0",
    "@loaders.gl/core": "3.1.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "electron-squirrel-startup": "^1.0.0",
    "ix": "4.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "webpack": "^5.0.0"
  },
  "workspaces": [
    "rapidsai/node_modules/@rapidsai/core",
    "rapidsai/node_modules/@rapidsai/cuda",
    "rapidsai/node_modules/@rapidsai/cudf",
    "rapidsai/node_modules/@rapidsai/cugraph",
    "rapidsai/node_modules/@rapidsai/cuml",
    "rapidsai/node_modules/@rapidsai/cuspatial",
    "rapidsai/node_modules/@rapidsai/deck.gl",
    "rapidsai/node_modules/@rapidsai/glfw",
    "rapidsai/node_modules/@rapidsai/io",
    "rapidsai/node_modules/@rapidsai/jsdom",
    "rapidsai/node_modules/@rapidsai/rmm",
    "rapidsai/node_modules/@rapidsai/sql",
    "rapidsai/node_modules/@rapidsai/webgl",
    "rapidsai/node_modules/wrtc"
  ]
}
